module Day02 exposing (PasswordSpec, input, part1, part2)

import Array
import List.Extra as List


type alias PasswordSpec =
    { min : Int
    , max : Int
    , letter : Char
    , password : String
    }


part1 : List PasswordSpec -> Int
part1 inputs =
    inputs
        |> List.count
            (\ps ->
                ps.password
                    |> String.toList
                    |> List.count ((==) ps.letter)
                    |> (\b -> b >= ps.min && b <= ps.max)
            )


part2 : List PasswordSpec -> Int
part2 inputs =
    inputs
        |> List.count
            (\ps ->
                let
                    chars =
                        ps.password |> String.toList |> Array.fromList

                    c1 =
                        Array.get (ps.min - 1) chars == Just ps.letter

                    c2 =
                        Array.get (ps.max - 1) chars == Just ps.letter
                in
                c1 /= c2
            )


